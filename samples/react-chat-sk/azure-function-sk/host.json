{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "api",
            "cors": {
                "allowedOrigins": ["https://*.sharepoint.com"],
                "supportCredentials": true
            }
        }
    }
}